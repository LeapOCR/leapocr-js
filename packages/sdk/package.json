{
  "name": "leapocr",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Official JavaScript/TypeScript SDK for LeapOCR API",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/leapocr/leapocr-js#readme",
  "bugs": {
    "url": "https://github.com/leapocr/leapocr-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leapocr/leapocr-js.git"
  },
  "author": "LeapOCR <support@leapocr.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "generate": "orval",
    "prepublishOnly": "pnpm run build",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "bumpp": "^10.3.1",
    "minimatch": "^10.1.1",
    "orval": "^7.16.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.16.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
